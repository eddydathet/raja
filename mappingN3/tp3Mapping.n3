map:database a d2rq:Database;
d2rq:jdbcDriver "com.mysql.jdbc.Driver";
d2rq:jdbcDSN "jdbc:mysql://localhost/VIRUS";
d2rq:username "root";
d2rq:password "audreybd";
.


# Table Transmission :
map:Transmission a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "http://www.lirmm.fr/virus#@@Transmission.modeT|urlify@@";
	d2rq:class v:Transmission;
	.
	
# Table Virus : 
map:Virus a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "http://www.lirmm.fr/virus#@@Virus.abrev@@";
	d2rq:class v:Virus;
	.

map:nomV a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Virus;
	d2rq:property v:nomV;
	d2rq:uriPattern "http://www.lirmm.fr/virus#@@Virus.NomV|urlify@@";
	.

map:typeGenome a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Virus;
	d2rq:property v:typeGenome;
	d2rq:uriPattern "http://www.lirmm.fr/virus#@@Virus.typeGenome@@";
	.
	
map:nomFa a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Virus;
	d2rq:property v:nomFa;
	d2rq:uriPattern "http://www.lirmm.fr/virus#@@Virus.nomFa@@";
	.


# Table Virus_Transmission :
# relation n:m
map:Virus_Transmission a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:Virus;
	d2rq:property v:Virus_Transmission;
	d2rq:refersToClassMap map:Transmission;
	d2rq:join "Virus_Transmission.abrevV = Virus.abrev";
	d2rq:join "Virus_Transmission.modeT = Transmission.modeT";
	.



