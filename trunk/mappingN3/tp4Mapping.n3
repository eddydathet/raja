# Table Maladie :
map:MALADIE a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "http://www.lirmm.fr/maladie#@@MALADIE.NOM_MALADIE|urlify@@";
	d2rq:class v:MALADIE;
	.
	
# Table Facteur : 
map:FACTEUR a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "http://www.lirmm.fr/maladie#@@FACTEUR.ABREV_FACTEUR|urlify@@";
	d2rq:class v:FACTEUR;
	.
	
map:NOMFACTEUR a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FACTEUR;
	d2rq:property v:NOMFACTEUR;
	d2rq:uriPattern "http://www.lirmm.fr/maladie#@@FACTEUR.NOM_FACTEUR|urlify@@";
	.
	
map:TYPEFACTEUR a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FACTEUR;
	d2rq:property v:TYPEFACTEUR;
	d2rq:uriPattern "http://www.lirmm.fr/maladie#@@FACTEUR.TYPE_FACTEUR|urlify@@";
	.
	
# Table APrOrigine :
# relation n:m
map:APRORIGINE a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "http://www.lirmm.fr/maladie#@@APRORIGINE.ABREVF|urlify@@_@@APRORIGINE.SOUCHE|urlify@@";
	d2rq:class v:APRORIGINE;
	.
	
map:ESTORIGINEDE a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:APRORIGINE;
	d2rq:property v:ESTORIGINEDE;
	d2rq:refersToClassMap map:MALADIE;
	d2rq:join "APRORIGINE.NOMM = MALADIE.NOM_MALADIE";
	.
	
map:ESTFACTEURDE a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:FACTEUR;
	d2rq:property v:ESTFACTEURDE;
	d2rq:refersToClassMap map:APRORIGINE;
	d2rq:join "APRORIGINE.ABREVF = FACTEUR.ABREV_FACTEUR";
	.
